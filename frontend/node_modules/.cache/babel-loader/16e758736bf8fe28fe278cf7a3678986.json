AAA;AACA;AACA;;AACA,MAAA,WAAA,CAAA,QAAA,CAAA,CAAA,EAAA,SAAA,CAAA,GAAA,CAAA,QAAA;AACA;;AACA,IAAA,CAAA,CAAA,MAAA,CAAA,aAAA,CAAA,SAAA,CAAA,GAAA,CACA,iBADA,EAEA,YAFA,EAGA,QAHA,EAIA,aAJA;AAMA,GA1BA;AA2BA,CA5BA;AA8BA,QAAA,CAAA,cAAA,CAAA,aAAA,EAAA,KAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,YADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,SAAA,EAAA,SADA;AAEA,IAAA,YAAA,EAAA,YAFA;AAGA,IAAA,aAAA,EAAA,aAHA;AAIA,IAAA,SAAA,EAAA;AAJA;AAFA,CAAA","sourcesContent":["<template>\n  <div class=\"admin-pages\">\n    <page-title\n      icon=\"fa fa-cogs\"\n      main=\"Administração do sistema\"\n      sub=\"Cadastros e Cia\"\n      class=\"font-mono text-4xl\"\n    />\n    <!-- component -->\n    <div class=\"w-1/2 mx-auto mt-4 rounded\">\n      <!-- Tabs -->\n      <ul id=\"tabs\" class=\"inline-flex w-full px-1 pt-2\">\n        <li\n          class=\"\n            px-4\n            py-2\n            -mb-px\n            font-semibold\n            text-gray-800\n            border-b-2 border-blue-400\n            rounded-t\n            opacity-50\n          \"\n        >\n          <a id=\"default-tab\">\n            <article-admin/>\n          </a>\n        </li>\n        <li class=\"px-4 py-2 font-semibold text-gray-800 rounded-t opacity-50\">\n          <a>\n            <category-admin/>\n          </a>\n        </li>\n        <li class=\"px-4 py-2 font-semibold text-gray-800 rounded-t opacity-50\">\n          <a>\n            <user-admin />\n          </a>\n        </li>\n        <li class=\"px-4 py-2 font-semibold text-gray-800 rounded-t opacity-50\">\n          <a>Others</a>\n        </li>\n      </ul>\n\n      <!-- Tab Contents -->\n      <div id=\"tab-contents\">\n        <div id=\"first\" class=\"p-4\">First tab</div>\n        <div id=\"second\" class=\"hidden p-4\">Second tab</div>\n        <div id=\"third\" class=\"hidden p-4\">Third tab</div>\n        <div id=\"fourth\" class=\"hidden p-4\">Fourth tab</div>\n      </div>\n    </div>\n  </div>\n</template>\n\n\n<script>\nimport PageTitle from \"../template/PageTitle.vue\";\nimport ArticleAdmin from \"./ArticleAdmin.vue\";\nimport CategoryAdmin from \"./CategoryAdmin.vue\";\nimport UserAdmin from \"./UserAdmin.vue\";\n\nlet tabsContainer = document.querySelector(\"#tabs\");\n\nlet tabTogglers = tabsContainer.querySelectorAll(\"a\");\nconsole.log(tabTogglers);\n\ntabTogglers.forEach(function (toggler) {\n  toggler.addEventListener(\"click\", function (e) {\n    e.preventDefault();\n\n    let tabName = this.getAttribute(\"href\");\n\n    let tabContents = document.querySelector(\"#tab-contents\");\n\n    for (let i = 0; i < tabContents.children.length; i++) {\n      tabTogglers[i].parentElement.classList.remove(\n        \"border-blue-400\",\n        \"border-b\",\n        \"-mb-px\",\n        \"opacity-100\"\n      );\n      tabContents.children[i].classList.remove(\"hidden\");\n      if (\"#\" + tabContents.children[i].id === tabName) {\n        continue;\n      }\n      tabContents.children[i].classList.add(\"hidden\");\n    }\n    e.target.parentElement.classList.add(\n      \"border-blue-400\",\n      \"border-b-4\",\n      \"-mb-px\",\n      \"opacity-100\"\n    );\n  });\n});\n\ndocument.getElementById(\"default-tab\").click();\n\nexport default {\n  name: \"AdminPages\",\n  components: {\n    PageTitle,\n    ArticleAdmin,\n    CategoryAdmin,\n    UserAdmin,\n  },\n};\n</script>\n\n<style>\n</style>\n"],"sourceRoot":"src/components/admin"}]}